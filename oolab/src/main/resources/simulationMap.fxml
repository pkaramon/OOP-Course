<!-- simulationMap.fxml -->
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
           fx:controller="agh.ics.oop.presenter.SimulationPresenter">
    <HBox>
        <VBox>
            <LineChart fx:id="animalCountChart">
                <xAxis><NumberAxis label="Dzień"/></xAxis>
                <yAxis><NumberAxis label="Liczba Zwierząt"/></yAxis>
            </LineChart>
            <!-- Możesz dodać więcej wykresów tutaj -->
        </VBox>
        <StackPane fx:id="mapContainer">
            <!-- Reszta Twojego GUI -->
        </StackPane>
    </HBox>
</StackPane>
